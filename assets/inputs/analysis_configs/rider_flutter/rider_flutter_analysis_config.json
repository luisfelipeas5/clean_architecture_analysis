{
  "path": "/Users/luisfelipeas5/Projects/rider_flutter/lib/src/",
  "exclude": [
    "/utils",
    ".DS_Store",
    ".*_module.dart",
    "shared/core/"
  ],
  "componentTypes": [
    {
      "name": "entity",
      "order": 0,
      "patterns": [
        {
          "regex": "modules/([A-Za-z0-9_]*)/domain/(entity|entities)/",
          "name": "modules/${1}/entities"
        },
        {
          "regex": "shared/domain/(entity|entities)/",
          "name": "shared/entities"
        }
      ]
    },
    {
      "name": "useCase",
      "order": 1,
      "patterns": [
        {
          "regex": "modules/([A-Za-z0-9_]*)/(domain|doman)/(usecases|usecase|use_case|use_cases)/",
          "name": "modules/${1}/use_cases"
        },
        {
          "regex": "shared/domain/(usecases|usecase|use_case|use_cases)/",
          "name": "shared/use_cases"
        },
        {
          "regex": "modules/([A-Za-z0-9_]*)/domain/(repositories|repository)/",
          "name": "modules/${1}/use_cases"
        },
        {
          "regex": "shared/domain/(repositories|repository)/",
          "name": "shared/use_cases"
        }
      ]
    },
    {
      "name": "interfaceAdapter",
      "order": 2,
      "patterns": [
        {
          "regex": "modules/([A-Za-z0-9_]*)/data/(repositories|respositories)/",
          "name": "modules/${1}/repositories"
        },
        {
          "regex": "shared/data/(repositories|respositories)/",
          "name": "shared/repositories"
        }
      ]
    },
    {
      "name": "frameworkOrDriver",
      "order": 3,
      "patterns": [
        {
          "regex": "modules/([A-Za-z0-9_]*)/presentation/",
          "name": "modules/${1}/presentation"
        },
        {
          "regex": "modules/([A-Za-z0-9_]*)/data/(data_sources|datasources|models|model)/",
          "name": "modules/${1}/data_sources"
        },
        {
          "regex": "shared/presentation/",
          "name": "shared/presentation"
        },
        {
          "regex": "shared/widgets/",
          "name": "shared/presentation"
        },
        {
          "regex": "shared/data/(data_sources|datasources|models|model)/",
          "name": "shared/data_sources"
        },
        {
          "regex": "feature_core/([A-Za-z0-9_]*)/",
          "name": "feature_core/${1}"
        }
      ]
    }
  ]
}